<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <template id="report_barang_pdf">
        <t t-call="web.html_container">
            <t t-call="web.internal_layout">

                <div class="page">
                    <h1>Daftar Barang</h1>
                    <t t-foreach="docs" t-as="doc">
                        <div class="row">
                            <div class="col-4"> Kode barang : </div>
                            <div class="col-8"><span t-field="doc.kode_barang"/></div>
                        </div>
                    </t>
                </div>
            </t>
        </t>
    </template>
</odoo>