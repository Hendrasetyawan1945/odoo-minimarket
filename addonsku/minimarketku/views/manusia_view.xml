<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="manusia_view_tree" model="ir.ui.view">
        <field name="name">manusia.view.tree</field>
        <field name="model">minimarket.manusia</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name"/>
                <field name="gender"/>
                <field name="alamat"/>
            </tree>
        </field>
    </record>

    <record id="manusia_view_form" model="ir.ui.view">
        <field name="name">manusia.view.form</field>
        <field name="model">minimarket.manusia</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="gender"/>
                        <field name="alamat"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="manusia_action" model="ir.actions.act_window">
        <field name="name">manusia_action</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">minimarket.manusia</field>
        <field name="view_mode">tree,form</field>
    </record>
    <!-- <menuitem
        id="manusia_menu"
        name="manusia"
        action="manusia_action"
        parent="karyawan_root"
        sequence="10" /> -->

</odoo>
